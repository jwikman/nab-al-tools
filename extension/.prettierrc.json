{
  "parser": "typescript",
  "endOfLine": "lf",
  "overrides": [
    {
      "files": [
        "**/*.md"
      ],
      "options": {
        "parser": "markdown",
        "endOfLine": "lf"
      }
    },
    {
      "files": [
        "**/*.js"
      ],
      "options": {
        "parser": "babel",
        "endOfLine": "lf"
      }
    },
    {
      "files": "**/*.json",
      "options": {
        "parser": "json-stringify",
        "endOfLine": "lf"
      }
    },
    {
      "files": "**/*.css",
      "options": {
        "parser": "css",
        "endOfLine": "lf"
      }
    }
  ]
}
