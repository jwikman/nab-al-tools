{
  "extends": "@istanbuljs/nyc-config-typescript",
  "require": [
    "ts-node/register",
    "source-map-support/register"
  ],
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test/*",
    "src/cli/*",
    "src/Logging/*.ts",
    "src/extension.ts",
    "src/DebugTests.ts",
    "src/PowerShell.ts",
    "src/NABfunctions.ts",
    "src/XlfHighlighter.ts",
    "src/Telemetry/Telemetry.ts",
    "src/PowerShellFunctions.ts",
    "src/Template/TemplatePanel.ts",
    "src/XliffEditor/XliffEditorPanel.ts",
    "src/PermissionSet/PermissionSetNamePanel.ts"
  ],
  "all": true,
  "reporter": [
    "text-summary",
    "lcov",
    "text"
  ],
  "check-coverage": true,
  "report-dir": ".coverage",
  "statements": 90,
  "branches": 73,
  "functions": 93,
  "lines": 89
}
