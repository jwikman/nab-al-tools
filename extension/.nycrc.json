{
  "extends": "@istanbuljs/nyc-config-typescript",
  "require": [
    "ts-node/register",
    "source-map-support/register"
  ],
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test/*",
    "src/cli/*",
    "src/Template/*",
    "src/Logging/*.ts",
    "src/PermissionSet/*",
    "src/XliffEditor/*",
    "src/extension.ts",
    "src/NABfunctions.ts",
    "src/Telemetry.ts",
    "src/XlfHighlighter.ts",
    "src/DebugTests.ts",
    "src/PowerShell.ts",
    "src/PowerShellFunctions.ts"
  ],
  "all": true,
  "reporter": [
    "text-summary",
    "lcov",
    "text"
  ],
  "check-coverage": true,
  "report-dir": ".coverage",
  "statements": 82,
  "branches": 65,
  "functions": 84,
  "lines": 82
}
