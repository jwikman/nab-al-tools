{
  "extends": "@istanbuljs/nyc-config-typescript",
  "require": [
    "ts-node/register",
    "source-map-support/register"
  ],
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test/*",
    "src/NABfunctions.ts",
    "src/Telemetry.ts",
    "src/XlfHighlighter.ts"
  ],
  "reporter": [
    "text-summary",
    "lcov"
  ],
  "check-coverage": true,
  "report-dir": ".coverage",
  "statements": 0, // TODO: Revert
  "branches": 0,
  "functions": 0,
  "lines": 0
}
