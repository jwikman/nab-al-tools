{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2019",
    "outDir": "out",
    "lib": [
      "ES2019",
      "dom",
      "es2018.regexp",
      "ES2020.String"
    ],
    "sourceMap": true,
    "rootDir": "src",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictBindCallApply": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "strict": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedParameters": true
  },
  "exclude": [
    "node_modules",
    ".vscode-test"
  ]
}
