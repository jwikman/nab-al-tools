{
  "mappings": [
    {
      "description": "The name of the app",
      "example": "NAB Kxxx Modifications",
      "default": "",
      "renameFiles": [
        {
          "path": "/NAB_PTE_TEMPLATE.code-workspace",
          "match": "NAB_PTE_TEMPLATE",
          "replaceSpaces": true,
          "replaceSpacesWith": "-"
        }
      ],
      "placeholderSubstitutions": [
        {
          "path": "**/*",
          "match": "[NAB_APP]"
        }
      ]
    },
    {
      "description": "The App Id",
      "example": "11112222-3333-4444-5555-666677778888",
      "default": "$(guid)",
      "placeholderSubstitutions": [
        {
          "path": "**/*",
          "match": "[NAB_APP_GUID]"
        }
      ]
    },
    {
      "description": "The first object id reserved for this app",
      "example": "50000",
      "default": "50000",
      "placeholderSubstitutions": [
        {
          "path": "**/*",
          "match": "[NAB_RANGE_START]"
        }
      ]
    },
    {
      "description": "The first object id, this will be increased for each match",
      "example": "50000",
      "default": "50000",
      "autoIncrement": true,
      "placeholderSubstitutions": [
        {
          "path": "**/*",
          "match": "[NAB_OBJECT_ID]"
        }
      ]
    },
    {
      "description": "The last object id reserved for this app",
      "example": "50099",
      "default": "50099",
      "placeholderSubstitutions": [
        {
          "path": "**/*",
          "match": "[NAB_RANGE_END]"
        }
      ]
    }
  ],
  "createXlfLanguages": [
    "sv-SE",
    "da-DK"
  ]
}