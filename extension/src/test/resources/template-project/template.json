{
  "mappings": [
    {
      "description": "The name of the app",
      "example": "NAB Kxxx Modifications",
      "default": "",
      "renameFile": [
        {
          "path": "/NAB_PTE_TEMPLATE.code-workspace",
          "match": "NAB_PTE_TEMPLATE"
        }
      ],
      "searchAndReplace": [
        {
          "path": "**/*",
          "match": "[NAB_APP]"
        }
      ]
    },
    {
      "description": "The App Id",
      "example": "11112222-3333-4444-5555-666677778888",
      "default": "$(guid)",
      "searchAndReplace": [
        {
          "path": "**/*",
          "match": "[NAB_APP_GUID]"
        }
      ]
    },
    {
      "description": "The prefix to use for this app",
      "example": "ABC ",
      "default": "NAB ",
      "searchAndReplace": [
        {
          "path": "**/*",
          "match": "[NAB_PREFIX]"
        }
      ]
    },
    {
      "description": "The first object id reserved for this app",
      "example": "50000",
      "default": "50000",
      "searchAndReplace": [
        {
          "path": "**/*",
          "match": "[NAB_RANGE_START]"
        }
      ]
    },
    {
      "description": "The last object id reserved for this app",
      "example": "50100",
      "default": "50100",
      "searchAndReplace": [
        {
          "path": "**/*",
          "match": "[NAB_RANGE_END]"
        }
      ]
    }
  ],
  "createXlfLanguages": [
    "sv-SE",
    "da-DK"
  ],
  "renumberObjects": true
}